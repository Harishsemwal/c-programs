/*WACP to print all the ASCII value and their equivalent character using
while loop. The ASCII value vary form 0 to 255. */ 
#include <stdio.h>
int main()
{
    int i;
    while(i<=255)
    {
        printf("%c:%d\n",i,i);
        i++;    
    }
    return 0;
}
/*Temprature of city in Fahrenheit degrees is input through the keyboard.
 write a programe to convert this Temprature into Centigrated degrees.*/
 //if Fahrenheit to Centigrated = (5/9*Centigrated)+32
 //if Centigrated to Fahrenheit = (5/9*Fahrenheit)-32

 #include<stdio.h>
 int main()
 {
    float Fahrenheit,Centigrated;
    printf("Please Enter thr Temprature of City:");
    scanf("%f",&Fahrenheit);
    Centigrated=(5/9*Fahrenheit)-32;
    printf("The Temprature in Centigrated is:%f\n",Centigrated);
    return 0;
 }
/*Write a programe to enter the user wants. At the end it should display the count of positive,negative and zeros entered.*/
 #include <stdio.h>

int main()

{ char ch;

int i,n=0,p=0,z=0;

do

{

printf("Enter an integer: \n");

scanf("%d",&i);

if(i>0)

p++;

else if(i<0)

n++;

else

z++;

printf("Do you want to continue? y/n \n");

scanf(" %c",&ch);

}while(ch=='y' || ch=='Y');

printf("Count of +ve , -ve and zeroes entered are %d , %d and %d respectively.",p,n,z);

return 0;

}/*Consider a Character system in which there are notes of six denominations,Rs.1,Rs.2,Rs.5,Rs.10,Rs.50,Rs.100.if a sum of Rs.N is entered through the keyboard,write a programe to compute the smallest number of Notes that will combine to give Rs.N.*/
#include<stdio.h>
int main()
{
    int ammount,a,b,c,d,e,f,total;
    printf("Enter The ammount:");
    scanf("%d",&ammount);
    a=ammount/100;
    ammount=ammount%100;
    b=ammount/50;
    ammount=ammount%50;
    c=ammount/10;
    ammount=ammount%10;
    d=ammount/5;
    ammount=ammount%5;
    e=ammount/2;
    ammount=ammount%2;
    f=ammount/1;
    ammount=ammount%1;
    total=a+b+c+d+e+f;
    printf("Smallest Number Of Notes:%d\n",total);
    return 0;
}/* the Distance Between Two cities (in Km) is input through the keyboard.Write
 a programe to convert and print this distance in meters,feet,inches and
 centemeter.*/
#include<stdio.h>
int main()
{
    float distance,meter,feet,inches,centemeter;
    printf("Enter the distance between Two citys in km citys is:");
    scanf("%f",&distance);
    meter=distance*1000;
    centemeter=meter*100;
    inches=centemeter/2.54;
    feet=inches/12;
    printf("The distance between two city in km:%f\n",distance);
    printf("The distance between two city in meter:%f\n",meter);
    printf("The distance between two city in centemeter:%f\n",centemeter);
    printf("The distance between two city in feet:%f\n",feet);
     printf("The distance between two city in inches:%f\n",inches);
    return 0;
}
