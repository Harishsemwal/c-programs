}/*Any year is input through the Keybord.write a programe to determine whether the the is leap
year or not.*/
#include<stdio.h>
int main()
{
    int year;
    printf("Enter The Year:");
    scanf("%d",&year);
    if(year%400==0)
    {
        printf("Year is leap Year");
    }
    if(year%100==0)
    {
        printf("Year is not a Leap Year");
    }
    if(year%4==0)
    {
        printf("Year is Leap Year");
    }
    return 0;
}#include<stdio.h>
int main()
{
    int age1,age2,age3; 
    printf("Enter the age of Ram:");
    scanf("%d",&age1);
    printf("Enter The age of Shyam:");
    scanf("%d",&age2);
    printf("Enter The age of Ajay:");
    scanf("%d",&age3);
    if(age1<=age2)
        
        if(age1<=age3)
        {
        printf("Ram is Youngest Brother!!!");
        }
        else{
            printf("Ajay is Youngest Brother!!!");
      }  
    else
    if(age2<=age3)
    {
        printf("Shyam is Youngest Brother!!!");
    }
    else
        {
         printf("Ajay is Youngest Brother!!!");
        }
    return 0;
}
//Number is Prime Or Not//
#include<stdio.h>
int main()
{
    int n,i;
    printf("Enter The Number:");
    scanf("%d",&n);
    for(i=2;i<n;i++)
    {
        if(n%i==0)
        {
            printf("Not Prime");
        break;
        }
    }
    if(n==i)
    {
        printf("prime");
    }
   
    return 0;
}
