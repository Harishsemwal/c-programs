}/*write a programe to find The factorial Value of N number enterend Through
the keyboard.*/
#include<stdio.h>
int main()
{
   int num,fact,i;
   printf("Enter the Number:");
   scanf("%d",&num);
   fact=i=1;
   while(i<=num)
   {
       fact=fact*i;
       i++;
   }
   printf("The factorial Value %d : %d",num,fact);
   return 0;
}/*write a programe to find a Factorial Value of any number enter through the Keyboard.*/
#include<stdio.h>
int main()
{
	int fact,i,num;
	printf("Enter The Number");
	scanf("%d",&num);
	fact=i=1;
	while(i<=num)
	{
		fact=fact*i;
		i++;
	}
	printf("The Number is %d and factorial is:%d",num,fact);
	return 0;
}/*if lenght of threee side of tringle are input through the keyboard,
write a programe to find the area of the tringle.
//wrtten by-Harish Semwal*/
#include<stdio.h>
#include<math.h>
int main()
{
    float a,b,c,s,area;
    printf("Enter the three different input of tringle:");
    scanf("%f %f %f",&a,&b,&c);
    s=(a+b+c)/2;
    area=sqrt(s*(s-a)*(s-b)*(s-c));
  printf("The area of tringle is:%f\n",area);
    return 0;
}/*if lenght of threee side of tringle are input through the keyboard,
