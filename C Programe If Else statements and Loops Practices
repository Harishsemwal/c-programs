}/*Two number are Enter Through the keyboard write a progrmame to find the Value of one
number raised to the power of another*/
#include<stdio.h>
int main()
{
	float x,power;
	int y,i;
	printf("Enter Two Numbers:");
	scanf("%f %d",&x,&y);
	power=i=1;
	while(i<=y)
	{
		power=power*x;
		i++;
	}
	printf("%f to the power %d is %f\n",x,y,power);
	return 0;
}/*Two number are entered through the keyboard. Write a programe to find the Value of one number raised
to the power of another.*/
#include<stdio.h>
int main()
{
    int power,a,b,i;
    printf("Enter the Two different number:");
    scanf("%d%d",&a,&b);
    power=i=1;
    while(i<=b)
    {
        power=power*a;
        i++;
    }
    printf("%d the power %d is:%d",a,b,power);
   return 0;
}/*Write a program to find the range of a set of number entered through the keyboard range is
the difference between the smallest and biggest number in the  list*/
#include<stdio.h>
int main()
{
    int small,big,range,num,limit;
    printf("Enter the limit:");
    scanf("%d",&limit);
    printf("Enter %d number:",limit);
    scanf("%d",&num);
    small=big=num;
    limit=limit-1;
    while(limit)
    {
        scanf("%d",&num);
        if(num>big)
        {
            big=num;
        }
        if(num>small)
        {
            big=small;
        }
        limit--;
    }
    range=big-small;
    printf("small num:%d\n",big);
    printf("Range is %d\n",range);
    return 0;
}/*if a five number is input through the Keyboard,write a programe to reverse the Number.*/
#include<stdio.h>
int main()
{
    int n,n1,n2,n3,n4,n5,reverse;
    printf("Enter The Five diffrent Number:");
    scanf("%d",&n);
    n5=n%10;
    n=n/10;
    n4=n%10;
    n=n/10;       
    n3=n%10;
    n=n/10;
    n2=n%10;
    n=n/10;
    n1=n%10;
    n=n/10;
    reverse=n5*10000+n4*1000+n3*100+n2*10+n1*1;
    printf("The reverse Number is:%d\n",reverse);
    return 0;
}/*If a charchter is entered Through the keyboard,write a programe to determine whether
the charchter is capital latter small case latter ,a digit or a special symbol.*/
#include<stdio.h>
int main()
{
	int ch;
	printf("Enter The Chachter");
	scanf("%c",&ch);
	if(ch>=65&&ch<=90)
	{
		printf("Character & Capital Latter");
	}
	else if(ch>=97&&ch<=122)
	{
		printf("Character & Small Latter");
	}
	else if(ch>=48&&ch<=57)
	{
		printf("it is Number");
	}
	else
	{
		printf("Special Symbol");
	}
	return 0;
}/* Given Three Points (x1,y1),(x2,y2) and (x3,y3),write a programe to cheak, if the three Points 
fall on one Straigth line.*/
#include<stdio.h>
#include<math.h>
int main()
{
    int x1,x2,x3,y1,y2,y3;
    float AB,BC,AC;
    printf("Enter The Frist Two co-ordinate");
    scanf("%d %d",&x1,&y1);
    printf("Enter The Frist Two co-ordinate");
    scanf("%d %d",&x2,&y2);
    printf("Ender The Frist Two co-ordinate");
    scanf("%d %d",&x3,&y3);
    AB= sqrt(pow(x1-x2,2)+(pow(y2-y1,2)));
    BC= sqrt(pow(x2-x3,2)+(pow(y2-y1,2)));
    AC= sqrt(pow(x3-x1,2)+(pow(y3-y1,2)));
    printf("The AB:%f\n and BC:%f\n and AC:%f\n",AB,BC,AC);
    if(AC==AB+BC)
    {
        printf("AB+BC=AC");
        printf("Three Points Fall on One straight line");
    }
    else{
        printf("Three Points Is not One straight line");
    }
    return 0;
}//write a programe to sum of Even Number Number 1 to N.
#include<stdio.h>
int main()
{
    int i,num;
    int sum=0;
    printf("Enter the Value of Last Term OF Number:");
    scanf("%d",&num);
    for(i=0;i<=num;i+=2)
    {
        sum=sum+i;
    }
    {
    printf("%d\n",sum);
    }
    return 0;
}/*if a five digit number input through the keyboard write a progrme to calculate
the sum of its digit.(Hint:Use the modulus operator %)*/
#include<stdio.h>
int main()
{
    int a,b,c,d,e,sum;
    printf("Enter The five different number:");
    scanf("%d %d %d %d %d",&a,&b,&c,&d,&e);
    sum=a+b+c+d+e;
    a=a%sum;
    b=b%sum;
    c=c%sum;
    d=d%sum;
    e=e%sum;
    printf("The Sum of Five Number is:%d",sum);
    return 0;
