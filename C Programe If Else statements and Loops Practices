int main()
{
	char gender;
	int yos,qual,salary;
	printf("Enter The Gender,Year of Service ,qualification(0=G,1=PG),Salary");
	scanf("%c %d %d %d",&gender,&yos,&qual,&salary);
	{
	if(gender=='M'&&yos>=10&&qual=1)
	salary=11000;
	else if(gender=='M'&&yos>=10&&qual=0)||if(gender=='M'&&yos<10&&qual=1)
	salary=10000;
	else if(gender=='M'&&yos<10&&qual=0)
	salary=7000;
	if(gender=='F'&&yos>=10&&qual=1)
	salary=12000;
	else if(gender=='F'&&yos>=10&&qual=0)||if(gender=='F'&&yos<10&&qual=1)
	salary=9000;
	else if(gender=='F'&&yos<10&&qual=0)
	salary=6000;
	printf("The Salary of Emplyee is: %d",&salary)
	}
	return 0;
}/*if a length of three sides of Triangle are entered through the keyboard,write a programe to cheak
whether the tringle is valid or not The tringle if valid if the sum of two sides is grater
than largest of three sides.*/
#include<stdio.h>
int main()
{
	int s1,s2,s3,largeside,sum;
	printf("Enter The Three sides of tringle:");
	scanf("%d %d %d",&s1,&s2,&s3);
	if(s1>s2)
	{
		if(s1>s3)
		{
			sum=s2+s3;
			largeside=s1;
		}
		else
		{
			sum=s1+s2;
			largeside=s3;
		}	
	}
	else 
	{
		if(s2>s3)
		{
		sum=s1+s2;
		largeside=s2;
		}
		else
		{
		sum=s1+s2;
		largeside=s3;
		}
	}
	if(sum>largeside)
	{
		printf("Tringle is Valid tringle");
	}
	else
	{
		printf("Tringle is not a valid Tringle");
	}
	return 0;
	
}/*write a programe to claculate the overtime pay 10 employee. Overtime is paid at the
rate of Rs.120 per hour for every our worked avobe 40 hours. Assume that employees do
not work for fractorial part of an hour.*/
#include<stdio.h>
int main()
{
	float overpay;
	int hour,i=1;
	while(i<=10)
	{
		printf("Enter The Work Hours:");
		scanf("%d",&hour);
		if(hour>=40)
		overpay=(hour-40)*120;
		else 
		overpay=0;
		printf("The over hour:%d\nTime paid:%f\n",hour,overpay);
		i++;
	}
	return 0;
 } 
