/*write a programe to find the absolute value of number enterd through the keyboard.*/
#include<stdio.h>
int main()
{
    float N,n;
    printf("Enter The Number");
    scanf("%f",&N);
    if(N>0)
    {
        printf("%f",N);
    }
    else if(N<0)
    {
        n=(-1)*N;
        printf("%f",n);
    }
    else if(N==0)
    {
        printf("%f",N);
    }
    return 0;
}
/*Given the length and breadth of  */
#include<stdio.h>
int main()
{
    int length=5,breadth=4,area,perimeter;
    area=length*breadth;
    perimeter=2*(length+breadth);
    if(perimeter>area)
    {
        printf("Yes!! The perimeter is Grater than area:%d",perimeter);
    }
    else{
        printf("The area %d is Grater than perimeter %d",area,perimeter);
    }
    return 0;
}/*WACP to print all the ASCII value and their equivalent character using
