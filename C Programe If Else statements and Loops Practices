}/*The Marks optained by student in 5 different subject are input throught the keyboard 
The student gets a division as per the Following rules.
percentage above or equal 60-Frist Devision.
persentage between 50 and 59-Secound Devision.
persentage between 40 and 49-Third Devision.
persentage less than 40- fail*/
#include<stdio.h>
int main()
{
	int a,b,c,d,e,sum,persentage;
	printf("Enter The Five Subject");
	scanf("%d %d %d %d %d",&a,&b,&c,&d,&e);
	sum=a+b+c+d+e;
	persentage=sum/5;
	printf("The persentage of Five subject is:%d\n",persentage);
	printf("The Sum of Five Subject is:%d\n",sum);
	if(persentage>=65)
	{
		printf("Frist Devidion");
	}
	else if(persentage>=50&&persentage<=59)
	{
		printf("Secound Devision");
	}
	else if(persentage>=40&&persentage<=49)
	{
		printf("Third Devision");
	}
	else if(persentage<40)
	{
		printf("fail");
	}
	return 0;
 }/* Nested if/Else condition problem:if cost price and selling price of an item is input through
the keyboard, write a programe to determine whether the seller has made profit or inncurred loss.
also determine how much profit he made or loss he incurred.*/
#include<stdio.h>
int main()
{
    float costprice,sellingprice,profit,loss;
    printf("Enter The cost price of an item:");
    scanf("%f",&costprice);
    printf("Enter The selling price of an item:");
    scanf("%f",&sellingprice);
     loss=costprice-sellingprice;
     profit=sellingprice-costprice;
    if(sellingprice>costprice)
    printf("The profit of an item is:%f\n",profit);
    if(sellingprice<costprice)
    {
        printf("The loss of an item is:%f\n",loss);
    }
    if(sellingprice==costprice)
    {
        printf("No profit!! and No loss!!");
    }
    return 0;
}/****************************************************************************************
Write a program for a matchstick game being played between the computer and a user your
program should ensure that the computer always wins rules for the game are as follows.
-- There are 21 matchsticks.
-- The computer asked the player to pick 1 2 3 or 4 or match sticks.
-- After the person picks the computer does it peaking.
-- Whoever is forced to pick up the last matchstick loses the game.
*****************************************************************************************/
#include<std>
int main()
{
    int match_sticks = 21, user_choice, computer_choice;
    while(match_sticks>=1)
    {
        printf("Total Match Sticks: %d\n", match_sticks);
        printf("Pick up the match sticks between (1 to 4): ");
        scanf("%d", &user_choice);

        if(user_choice>4)
        {
            printf("Invalid Entry");
            break;
        }

        //for computer choice we are taking maximum value by which
        //we can get the max possible value of sticks which is 5
        computer_choice = 5 - user_choice;
        //with the help of this code we will take only 4 rounds to decide that user loses the match.
        printf("Computer picks up the %d match sticks.\n", computer_choice);
        match_sticks = match_sticks-user_choice-computer_choice;
        if(match_sticks==1)
        {
            printf("\nYou have been lost via computer.");
            break;
        }
    }
    return(0);
}//Write a program to receive an integer and find its octal equivalent.
#include<stdio.h>
int main()
{
    int num,r,res=0,oct=0,flag=0;
    printf("Enter the integar:");
    scanf("%d",&num);
    r=num;
    while(r!=0)
    {
        res=res*10+r%8;
        if(res==0)
        {
            flag=1;
        }
        r=r/8;
    }
    while(res!=0)
    {
        oct=oct*10+res%10;
        res=res/10;
    }
    if(flag==1)
    {
        oct=oct*10;
    }
    {
    printf("The octal of %d is %d",num,oct);
    }
    return 0;
}/*The length and breadth of rectangle and redius of a circle are input through the keyboard.
write a programe to calculate the area and perimeter of the rectangle,and the area and 
circumference of the circle.*/

#include<stdio.h>
int main()
{
    int length,breadth,area,radius;
    float PI=3.142,area_circle;
    printf("Enter The length perimeter of The rectangle:");
    scanf("%d",&length);
    printf("Enter The breadth perimeter of The rectangle:");
    scanf("%d",&breadth);
    printf("Enter the radius of Circle:");
    scanf("%d",&radius);
    area=length*breadth;
    area_circle=PI*radius*radius;
    printf("the area and perimeter of the rectangle:%d\n",area);
    printf("the area and circumference of the circle:%f\n",area_circle);
    return 0;
}#include<stdio.h>
